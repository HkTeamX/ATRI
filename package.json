{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "pnpm --filter @huan_kong/atri-example dev",
    "start": "pnpm --filter @huan_kong/atri-example start",
    "lint": "pnpm --filter @huan_kong/atri lint && pnpm --filter @huan_kong/atri-example lint",
    "type-check": "pnpm --filter @huan_kong/atri type-check && pnpm --filter @huan_kong/atri-example type-check",
    "build": "pnpm type-check && pnpm --filter @huan_kong/atri build",
    "publish": "pnpm lint && pnpm build && cd packages/core && npm publish",
    "docs:dev": "pnpm --filter @huan_kong/atri-docs dev",
    "docs:build": "pnpm --filter @huan_kong/atri-docs build",
    "docs:preview": "pnpm --filter @huan_kong/atri-docs preview"
  },
  "dependencies": {
    "jiti": "^2.5.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
